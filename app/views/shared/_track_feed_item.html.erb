<li id="<%= track_feed_item.id %>">
  <span class="name">
    <%= track_feed_item.name %>
  </span>
  <span class="links">
  	<%= link_to 'info', track_feed_item %>
	  <% if current_user?(track_feed_item.user) %>
	    | <%= link_to 'edit', edit_track_path(track_feed_item) %>
	    | <%= link_to "delete", track_feed_item, method: :delete,
	                                     data: { confirm: "You sure?" },
	                                     title: track_feed_item.name %>
	  <% end %>
  </span>
</li>