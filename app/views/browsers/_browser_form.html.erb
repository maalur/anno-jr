<%= form_for(@browser) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :data_download %>
  <%= f.text_area :data_download %>

  <%= f.label :citation %>
  <%= f.text_field :citation %>

  <%= f.label 'Genome fetcher link' %>
  <%= f.text_field :genome %>

  <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
<% end %>